{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}",
                "C:/MinGW/include",
                "C:/MinGW/lib/gcc/mingw32/*/include/c++",
                "C:/MinGW/lib/gcc/mingw32/*/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "__GNUC__",
                "__cdecl=__attribute__((__cdecl__))"
            ],
            "compilerPath": "C:\\MinGW\\bin\\g++.exe",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "windows-gcc-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}/**",
                    "C:/MinGW/include",
                    "C:/MinGW/lib/gcc/mingw32/*/include/c++"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "compilerArgs": [
                "-Wall",
                "-Wextra",
                "-std=c++17"
            ]
        }
    ],
    "version": 4
}